root = true

[*]
charset                  = utf-8
end_of_line              = lf
insert_final_newline     = true
trim_trailing_whitespace = true

[*.kt]
indent_style                    = space
indent_size                     = 4
ktlint_standard_property-naming = disabled

[*TestCase.kt]
ktlint_standard_function-naming = disabled


